            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));

        $(window).load(function () {
            $('.flexslider').flexslider({
                animation: "slide",
                pauseOnAction: true,
                pauseOnHover: true, keyboard: true,
                multipleKeyboard: false,
                mousewheel: true,
                pausePlay: true,
                pauseText: 'Pause',
                playText: 'Play',
            });
        });
    </script>

